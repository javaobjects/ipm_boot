    <style>
        rect.selected{
            stroke-dasharray: 1;
            stroke: #BD3613;
        }
        .graphContextmenu{
            /*position:absolute;*/
            z-index: 2;
            border-radius: 5px;
            /*display: none;*/
            position: absolute;
            list-style: none;
            background: #6e839b;
            /*opacity: 0.9;*/
            cursor: pointer;
            -webkit-padding-start: 0px;
            color:#fff;
            /*margin: 0;*/
            /*padding: 0;*/
            /*font-family: Helvetica Neue,Helvetica, Arial,sans-serif;*/
            /*font-size: 12px*/
        }
        .fontPtcolor{
            color:#fff;
            margin-right: 7px;
        }
        .white{
            color:#fff;
        }
        .graphContextmenu .white:hover .fontPtcolor{
            color:#2a6496
        }



        .mloadingDiv {
            position: absolute;
            background: #f3f8ff;
            opacity: 0.8;
            z-index: 10000;
        }
        .loadingDivChild {
            position: absolute;
            cursor: wait;
            width: auto;
            height: 57px;
            line-height: 57px;
            padding-left: 50px;
            padding-right: 5px;
            background: #fff url(../../images/gif.gif) no-repeat scroll 18px 20px;
            border: 2px solid #95B8E7;
            color: #696969;
            font-family: \'Microsoft YaHei\';
        }
        .exemple {
            width: 125px;
            height: 155px;
            position: absolute;
            right: 2%;
            bottom: 2%;
            border: 1px solid grey;
            border-radius: 5px;
            background: rgba(51, 71, 81, 0.5);
            color: white;
            text-align: center;
            font-size: 12px;
        }
        .exemple>ul {
            padding: 12% 0% 0% 0%;
        }
        .exemple>.media-list>h1 {
            text-decoration: underline;
            font-size: 13px;
            margin: 0;
            padding: 0;
        }
        .media {
            margin-top: 8px;
        }
        .exemple>ul>li:nth-of-type(1) {
            height: 20px;
        }
        .exemple>ul>li:nth-of-type(2) {
            height: 20px;
        }
        .exemple>.media-list>.media:last-child {
            margin-top: 10%;
        }
        .exemple>.media-list>.media:last-child>a>.media-object {
            margin-left: 25%;
        }
        .exemple>.media-list>.media:last-child>.media-body {
            margin-top: 9%;
            width: 80%;
            margin-left: 10%;
        }
        .serverImg,.portImg {
            position: relative;
            left:15px;
        }
        .saveTopoNameInput{
            background: #404040;
            color:#ddd;
            border-radius: 0;
            box-shadow: none;
            font-weight: 400;
            font-size: 12px;
            border: 1px solid rgba(255, 255, 255, 0.3);
            display: block;
            width: 100%;
            height: 34px;
            padding: 6px 12px;
            line-height: 1.42857143;
        }
    </style>
