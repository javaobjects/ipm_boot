.mloadingDiv{
    position:absolute;
    left:0;
    width:100%;
    top:0;
    background:#f3f8ff;
    opacity:0.8;
    filter:alpha(opacity=90);
    z-index:10000;
}
.loadingDivChild{
    position: absolute;
    cursor: wait;
    width: auto;
    height:57px;
    line-height:57px;
    padding-left: 50px;
    padding-right: 5px;
    background: #fff url(../../images/gif.gif) no-repeat scroll 18px 20px;
    border: 2px solid #95B8E7;
    color: #696969;
    font-family:\'Microsoft YaHei\';
}
.suspension{
    position:absolute;
    z-index: 2;
    background:#7e93ae;
    opacity:0.9;
    color:white;
    box-shadow:2px 2px 2px #8d9eb7,2px 2px 2px #ffffff;
    border-radius: 3px;
    text-align: center;
    display: none;
}
.suspension p{
    margin: 5px;
}
.function {
    margin-left: 60%;
}
.function ul {
    list-style:none;
}
.function ul li {
    float:left;
    margin-right: 7px;
}
div.panel[name=topo_toolbar] {
    right: 25px;
    top:10px;
}
div.panel {
    color: white;
    background: rgba(51, 122, 183, 0);
    position: absolute;
    /*display:inline-block;*/
}
.btn-group{
    position: relative;
    display: inline-block;
    vertical-align: middle;
}
.mybtn-group{height:30px;}
div.panel[name=topo_toolbar] span {
    display: inline;
}
.mypanel-body{
    padding: 0px;
}
div.panel[name=topo_toolbar] .input-group {
    display: inline-block;
}
div.panel[name=topo_toolbar] ul[name=search_mode] {
    border: solid 0px #568492;
    min-width: 0px;
    width: 100px;
    color: #fff;
    margin: 0px 0 0;
    padding: 0px 0;
    font-size: 14px;
    background-color: #7e93ae;
    text-align: center;
}
div.panel[name=topo_toolbar] ul[name=search_mode]> li > a{
    padding: 0px 0px;
}
.btn-sus{
    color: #fff;
    background-color: #7e93ae;
    border-color: #568492;
}
.close{
    /*position: absolute;*/
