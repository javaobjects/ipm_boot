}
.media {
    margin-top: 8px;
}
.exemple{
    width:125px;
    height: 155px;
    position: absolute;
    right: 2%;
    bottom: 2%;
    border:1px solid grey;
    border-radius: 5px;
    background:rgba(51, 71, 81, 0.5);
    color:white;
    text-align: center;
    font-size: 12px;
}
.exemple>ul{
    /*padding: 12% 11% 0% 8%;*/
    padding: 12% 0% 0% 0%;
}
.exemple>ul>li:nth-of-type(1){
    height: 20px;
}
.exemple>ul>li:nth-of-type(1)>a{
    margin-top:4px;
}
.exemple>ul>li:nth-of-type(2){
    height: 20px;
}
.exemple>ul>li:nth-of-type(2)>a{
    margin-top:4px;
}
.exemple>.media-list>h1{
    text-decoration: underline;
    font-size: 13px;
    margin: 0;
    padding: 0;
}
.exemple .media-list .media .pull-left .media-object{
    /*margin-left: 337%;*/
    position: relative;
    left: 21px;
}
.exemple>.media-list>.media:last-child{
    margin-top:10%;
}
.exemple>.media-list>.media:last-child>a>.media-object{
    margin-left: 25%;
}
.exemple>.media-list>.media:last-child>.media-body{
    margin-top: 9%;width: 80%;margin-left: 10%;
}
