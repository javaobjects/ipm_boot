#contextmenu{
    border-radius: 5px;
    display:none;
    position: absolute;
    list-style: none;
    background: #337ab7;
    background: #6e839b;
    opacity:0.9;
    cursor: pointer;
    margin:0;padding:0;
    font-family:Helvetica Neue,Helvetica, Arial,sans-serif;
