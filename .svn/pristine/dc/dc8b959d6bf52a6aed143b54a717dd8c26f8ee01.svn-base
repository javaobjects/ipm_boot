/*right CSS OVER*/
/***************内容区的样式OVER*********************/
/*将table的分页按钮变小*/
span.btn-group>button[data-toggle="dropdown"]{
    padding:0px;
}
/*将table的分页按钮变小*/
/*表格隐藏消失列的文字颜色*/
.dropdown-menu>li[role="menuitem"]>label{
    color:#000000 ;
}
/*表格隐藏消失列的文字颜色*/
/*table hover 时改变字体颜色*/
.table-hover>tbody>tr:hover{
    color:#fff;
}
.table-hover>tbody>tr:hover a{
    color:#fff;
}
/*table hover 时改变字体颜色*/
/*将表头字体变细*/
.th-inner{
    /*font-weight:100;*/
}
/*将表头字体变细*/
/*将日期插件的字体颜色调为黑色*/
.datetimepicker{
    color:#000000;
}
/*button自定义样式*/
.btnSelf:hover{
    color:#fcfcfc;
    background: rgba(189, 186, 186, 0.4);
}
/*时间回溯弹出设置*/
.popover {
    background: rgba(255,255,255,.9);
}
.popover-title {
    color: red;
    line-height: 10px;
    background: rgba(20,20,20,.8);
}
.popover-content {
    color: black;
}
/**********重置此CSS样式***********/
.pagination>li>a,.btn-default,.nav-tabs>li.active>a,.btnSelf,.input-group-addon{
    border: 1px solid rgba(255, 255, 255, 0.3);
    background: rgba(0,0,0,0.05);
    color:#fcfcfc;
}
.btn-default:hover{
    background: rgba(0,0,0,0.3);
    color:#fcfcfc;
}
.btn-default:focus{
    background: rgba(0,0,0,0.3);
    color:#fcfcfc;
}
.btn:focus {
    color:#fcfcfc;
}
.fixed-table-container{
    /*border: 1px solid #AAAAAA;*/
    border: none;
}
.fixed-table-container{
    background: rgba(0,0,0,0.35);
}
.table td, .table th {
    border-color: rgba(255,255,255,0.05)!important;
}
.bootstrap-table .table{
    border-bottom: 1px solid rgba(255,255,255,0.05);
}
.table-hover>tbody>tr:hover>td,.table-hover>tbody>tr:hover>th {
    background-color:#1F3E3A;
}
.table>thead>tr>td.active,.table>tbody>tr>td.active,.table>tfoot>tr>td.active,.table>thead>tr>th.active,.table>tbody>tr>th.active,.table>tfoot>tr>th.active,.table>thead>tr.active>td,.table>tbody>tr.active>td,.table>tfoot>tr.active>td,.table>thead>tr.active>th,.table>tbody>tr.active>th,.table>tfoot>tr.active>th {
    background-color:#1F3E3A;
}
.table>thead{
    background:rgba(0, 0, 0, 0.18);
}
.pagination>li>a:hover,.pagination>.active>a,.pagination>.active>a:hover{
    background-color:   #C0C0C0;
    border-color:   #C0C0C0;
}
.range {
    padding-top: 2px;
}
.form-control[disabled] {
    background: rgba(0,0,0,.05);
}
/******************setting页面的CSS OVER*******************/

/* 解决IE按钮框线问题 */
 .mainbody a:focus{outline:none;}
 
 .mainbody a:hover {
    color: #fff;
    text-decoration: none;
 }